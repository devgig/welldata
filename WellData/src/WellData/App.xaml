<Application x:Class="WellData.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WellData">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                    <ResourceDictionary  Source="pack://application:,,,/WellData.Ui;component/UiResources.xaml" />
                    <ResourceDictionary>
                        <local:AutoFacBootStrapper x:Key="Bootstrapper"/>
                    </ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
